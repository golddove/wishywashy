{
    "name": "WishyWashy",
    "version": "1.0",
    "manifest_version": 2,
    "description": "WishyWashy is a google chrome gift wishlist app",
    "browser_action": {
        "default_icon": "gift.png",
        "default_popup": "main.html",
        "default_title": "WishyWashy Wish List"
    },
    "permissions": [
        "storage",
        "identity",
        "tabs",
        "http://*/*",
        "https://*/*",
        "pageCapture",
    ],
    "background":{
        "scripts":["background.js"],
        "persistent":false
    },
    "oauth2" : {
        "client_id" : "114079785486-3fcts9m90q930f4u3ougvn3kun4261jh.apps.googleusercontent.com",
        "scopes":[
            "https://www.googleapis.com/auth/userinfo.email",
            "https://www.googleapis.com/auth/userinfo.profile"
        ]
    },
    "content_scripts": [
      {
        "matches": [
          "http://*/*"
        ],
        "js": [
          "select.js"
        ],
        "run_at": "document_start",
        "all_frames": true
      }
    ],
    "key" : "",
    "content_security_policy": "script-src 'self' https://cdn.firebase.com https://*.firebaseio.com; object-src 'self'"

}